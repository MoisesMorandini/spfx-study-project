{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "view-list-generic-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/viewListGeneric/ViewListGenericWebPart.js",
          "manifest": "./src/webparts/viewListGeneric/ViewListGenericWebPart.manifest.json"
        }
      ]
    },
    "new-employee-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/newEmployee/NewEmployeeWebPart.js",
          "manifest": "./src/webparts/newEmployee/NewEmployeeWebPart.manifest.json"
        }
      ]
    },
    "view-library-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/viewLibrary/ViewLibraryWebPart.js",
          "manifest": "./src/webparts/viewLibrary/ViewLibraryWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ViewListGenericWebPartStrings": "lib/webparts/viewListGeneric/loc/{locale}.js",
    "NewEmployeeWebPartStrings": "lib/webparts/newEmployee/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "ViewLibraryWebPartStrings": "lib/webparts/viewLibrary/loc/{locale}.js"
  }
}
